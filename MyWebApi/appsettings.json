{
    "ConnectionStrings": {
        "Default": "Server=localhost;Database=MyCustomDatabase;Trusted_Connection=True;"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        },
        "Filter": [
            {
                "Name": "ByExcluding",
                "Args": {
                    "expression": "@mt = 'An unhandled exception has occurred while executing the request.'"
                }
            }
        ],
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "./logs/log-.txt",
                    "rollingInterval": "Day"
                }
            }
        ]
    },
    "AdminAuthData": {
        "UserName": "prodadmin",
        "Password": "prodadmin"
    },
    "AllowedHosts": "*"
}
